{
   "name": "nuzlooke-client",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "start": "npm run dc:up && npm run serve",
      "serve": "vue-cli-service serve",
      "build": "vue-cli-service build",
      "dc:up": "docker-compose up -d",
